    "enzyme": "^2.7.1",
    "react-addons-test-utils": "^15.4.2",
    "react-app-rewired": "^1.4.0",
    "react-scripts-ts": "^2.12.0"
  },
  "dependencies": {
    "@types/react-dnd-html5-backend": "^2.1.8",
    "@types/react-tap-event-plugin": "^0.0.30",
    "firebase": "^4.1.3",
    "gh-pages": "^1.0.0",
    "husky": "^0.14.3",
    "konva": "^1.6.7",
    "lint-staged": "^4.2.3",
    "material-ui": "^0.18.6",
    "prettier": "^1.7.4",
    "react": "^15.6.1",
    "react-contenteditable": "^2.0.4",
    "react-dnd": "^2.4.0",
    "react-dnd-html5-backend": "^2.4.1",
